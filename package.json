{
  "name": "nx-stack",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx run-many --target=serve --projects=frontend,api",
    "start:ui": "nx serve frontend",
    "start:api": "nx serve api",
    "build": "nx build",
    "build:ui": "nx build frontend",
    "build:ui:prod": "nx build frontend --configuration=production",
    "build:api": "nx build api",
    "build:api:prod": "set NODE_ENV=production && nx build api --configuration=production",
    "test": "nx test",
    "deploy": "nx run infrastructure:deploy"
  },
